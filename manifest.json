{
    "manifest_version": 2,
    "name": "Booklet",
    "description": "See and organize your bookmarks as trees",
    "version": "1.0",
    "background": {
      "scripts": ["public/index.html"]
  },
    "chrome_url_overrides" : {
    	"newtab": "public/index.html"
    },
    "content_security_policy": "script-src 'self'; 'unsafe-eval'; object-src 'self'",
    "permissions": [
    	"bookmarks",
        "tabs",
        "unlimitedStorage",
        "management"
    ]
}


